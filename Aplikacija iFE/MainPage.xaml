<Page
    x:Class="Aplikacija_iFE.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel>
        <StackPanel Height="559">
            <Grid  Height="46" Margin="10,20,10,0">
                <Button x:Name="Camerareport" Content="Camera_report" Margin="0,0,0,10" Click="Camera_click" Width="120" Height="30"/>
                <Button x:Name="Navigacija" Content="Navigacija" HorizontalAlignment="Left"  Margin="220,0,0,16" Width="120" Height="30"/>
            </Grid>


            <Grid  Height="30" Margin="10,20,0,5">
                <Button x:Name="Menu" Content="Jedilnik" Click="Menu_Click" Width="120" Height="30"/>
                <Button Content="Profesorji" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="220,0,10,0" Width="120" Height="30" Click="Button_Click"/>
            </Grid>

            <Grid  Height="30" Margin="10,20,0,5">
                <Button Content="Nastavitve" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,0,230,0" Width="120" Height="30" Click="Button_Click_1"/>
            </Grid>
            <Grid  Margin="10,5,0,5"/>


        </StackPanel>
        <Frame/>
    </StackPanel>

</Page>
